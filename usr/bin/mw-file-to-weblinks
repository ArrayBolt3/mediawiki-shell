#!/bin/bash

#set -x
set -e

## TODO: no root check

file_name="$1"
test -r "$file_name"

tr -s '[:blank:]' '[\n*]' < "$file_name" | while IFS= read -r word; do
   #echo "word: '$word'"

   if [ "$word" = "" ]; then
      continue
   fi

   if link_result_orig=$(echo "$word" | grep --fixed-string 'https://') ; then
      echo "$link_result_orig"
   fi
done
